#pragma config(Sensor, S1,     lightSensor,    sensorEV3_Color)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

task main()
{
	while(SensorValue(lightSensor)>45)
	{
	motor[motorC]=50;
	motor[motorB]=50;
	
	wait1Msec(1);
	}

	motor[motorC]=-50;
	motor[motorB]=-50;
	wait1Msec(1000);
}
